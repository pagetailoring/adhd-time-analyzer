{
  "name": "adhd-time-analyzer",
  "version": "0.3.1",
  "author": "pagetailoring",
  "description": "Nuxt UI-based interface for the app",
  "private": false,
  "type": "module",
  "scripts": {
    "deploy": "tsx build/deploy.ts",
    "deploy:app": "pnpm run deploy _app",
    "deploy:demo": "pnpm run deploy demo--adhd-analizer",
    "deploy:dev": "pnpm run deploy _development",
    "deploy:stage": "pnpm run deploy _stage",
    "gen:app": "nuxi generate --dotenv ../.env.app",
    "gen:demo": "nuxi generate --dotenv ../.env.demo",
    "gen:dev": "nuxi generate --dotenv ../.env.development",
    "gen:stage": "nuxi generate --dotenv ../.env.stage",
    "gen:ts:app": "pnpm run gen:tsconfig ../.env.app",
    "gen:ts:demo": "pnpm run gen:tsconfig ../.env.demo",
    "gen:ts:dev": "pnpm run gen:tsconfig ../.env.development",
    "gen:ts:local": "pnpm run gen:tsconfig ../.env.local",
    "gen:ts:stage": "pnpm run gen:tsconfig ../.env.stage",
    "gen:tsconfig": "tsx build/gen-tsconfig.ts",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix",
    "nuxt:analyze": "nuxi analyze  --dotenv ../.env.demo",
    "nuxt:build": "nuxi build --dotenv ../.env.demo",
    "nuxt:clean": "nuxi cleanup",
    "nuxt:generate": "nuxi generate --dotenv ../.env.local",
    "nuxt:postinstall": "nuxi prepare --dotenv ../.env.local",
    "nuxt:preview": "nuxi preview --dotenv ../.env.local",
    "nuxt:test": "vitest",
    "nuxt:upgrade": "nuxi upgrade",
    "pre:dev": "pnpm run gen:ts:local && dotenv -e ../.env.local -- vitest run tests/dotEnv.spec.ts --run",
    "pre:test": "tsx build/test-app.ts",
    "pre:test:app": "pnpm run pre:test ../.env.app",
    "pre:test:demo": "pnpm run pre:test ../.env.demo",
    "pre:test:dev": "pnpm run pre:test ../.env.development",
    "pre:test:stage": "pnpm run pre:test ../.env.stage",
    "run:dev": " nuxi dev --dotenv ../.env.local",
    "run:host": "nuxi dev --host --dotenv ../.env.local",
    "run:typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "test:demo": "dotenv -e ../.env.demo -- vitest",
    "test:local": "dotenv -e ../.env.local -- vitest",
    "seperator": "echo '------------------------------------ ↙️ main scripts '",
    "typecheck": "run-s gen:ts:local run:typecheck",
    "up": "nuxi upgrade",
    "clean": "run-s nuxt:clean nuxt:postinstall",
    "app": "run-s gen:ts:app pre:test:app gen:app deploy:app",
    "demo": "run-s gen:ts:demo pre:test:demo gen:demo deploy:demo",
    "development": "run-s gen:ts:dev pre:test:dev gen:dev deploy:dev",
    "stage": "run-s gen:ts:stage pre:test:stage gen:stage deploy:stage",
    "host": "run-s pre:dev run:host",
    "dev": "run-s pre:dev run:dev"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.11.0",
    "@vueuse/nuxt": "^13.1.0",
    "nuxt": "^3.17.3",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.19.0",
    "@nuxtjs/html-validator": "^2.1.0",
    "@types/fs-extra": "^11.0.4",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.3.1",
    "execa": "^9.5.3",
    "fs-extra": "^11.3.0",
    "happy-dom": "^17.4.7",
    "nuxt-payload-analyzer": "^0.1.4",
    "playwright-core": "^1.52.0",
    "prettier": "^3.5.3",
    "rollup-plugin-license": "^3.6.0",
    "strip-json-comments": "^5.0.1",
    "tinyexec": "^1.0.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  }
}
